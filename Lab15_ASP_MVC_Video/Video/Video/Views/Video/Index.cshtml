@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>video</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/murkou.css" rel="stylesheet" type="text/css" />
    <style type="text/css">
        html, body {
            height: 100%;
            width: 100%;
        }

        body, td, tbody, table, tr, h2 {
            margin: 0px 0px 0px 0px;
            padding: 0px 0px 0px 0px;
            vertical-align: top;
        }
    </style>
</head>

<body>
    <table style="height:100%;width:100%">
        <tr>
            <td width="25%" style="vertical-align:top">
                <div style="margin: 10px 10px">
                    
                    <table>
                        <p>Список видеозаписей</p>
                        @Html.Partial("VideoList")
                    </table>
                </div>
            </td>
            <td width="75%" style="vertical-align:top">
                <iframe name="video_frame" style="width:100%;height:100%" />
            </td>
        </tr>
    </table>
</body>
</html>